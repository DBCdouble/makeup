/* @import "post-item/post-item-template.wxss"; */
@import "good-item/good-item-template.wxss";
 swiper{
     width:100%;
     height:352rpx;
 }
 /*less sass*/
 .post{
   background-color:#F5F5F5; 
 }
 swiper image{
  width:100%;
  height:352rpx;
 }
 .iconTemp{
  width:100%;
  height:166rpx;
  display:flex;
  flex-direction:row;
  background-color:#F5F5F5;
  padding-bottom:10rpx;
 }
 .iconImg{
  height:166rpx;
  flex:1;
  background-color:#ffffff;  
 }
 .DTemp{
  width: 100%;
  height: 710rpx;
  background-color:#F5F5F5;
  margin-bottom:30rpx;
 }
 .DTempPic{
  width:100%;
  height: 400rpx;
 }
 .DGoodlist{
   width: 100%;
   height: 310rpx;
   white-space: nowrap;
   background-color: #ffffff;
 }
 .DItem{
  width:200rpx;
  height: 310rpx;
  display: inline-block;
  background-color:#ffffff;
 }
 .DGoodPic{
  width: 100%;
  height: 220rpx;
 }
 .DTitle{
  width: 100%;
  height: 40rpx; 
  font-size: 24rpx;
  color:black;
  text-align: center;
  overflow: hidden;
  white-space: nowrap;
  text-overflow:ellipsis; 
 }
 .DPrice{
  width: 100%;
  height: 50rpx;
  font-size: 24rpx;
  color:#E41438;
  font-weight:bold;
  text-align: center;
 }
 .goodlist_title{
   width:100%;
   height: 99rpx;
   line-height: 99rpx;
   font-size:30rpx;
   color:#2F2F2F;
   background-color: #ffffff;
   text-align: center;
   font-family: PingFang-SC-Medium;
   font-weight: bold;
 }
 .good-container{
   display: flex;
   flex-wrap:wrap;
   justify-content: space-between;
   background-color: #F5F5F5;
   padding:10rpx 0;
 }